{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
      "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
      ],
      "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
      ]
}
