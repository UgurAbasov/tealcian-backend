    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "schemapack": "^1.4.2",
    "socket.io-json-parser": "^3.0.0",
    "socket.io-parser": "^4.2.4",
